{
  "version": 2,
  "buildCommand": "pip install --upgrade pip setuptools wheel && pip install -r requirements.txt",
  "routes": [
    {
      "src": "/health",
      "dest": "/api/index"
    },
    {
      "src": "/api/radiomics",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}
